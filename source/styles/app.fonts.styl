    content: "\ea10"
  &.arrow-up:before
    content: "\ea3a"
  &.arrow-down:before
    content: "\ea3e"
  &.google-plus:before
    content: "\ea88"
  &.facebook:before
    content: "\ea8c"
  &.twitter:before
    content: "\ea91"
  &.dribbble:before
    content: "\eaa6"
